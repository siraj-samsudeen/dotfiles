{
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "git.autofetch": true,
  "explorer.confirmDragAndDrop": false,
  "git.ignoreRebaseWarning": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 100,
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "never",
  },
  "prettier.singleQuote": true,
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit",
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
  },
  "notebook.formatOnSave.enabled": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
  },
  "yaml.schemas": {
    "file:///Users/siraj/.vscode/extensions/atlassian.atlascode-3.0.0/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml",
  },
  "atlascode.jira.enabled": false,
  "redhat.telemetry.enabled": true,
  "atlascode.bitbucket.enabled": false,
  "workbench.startupEditor": "none",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.terminalLinks.enabled": false,
  "gitlens.views.commitDetails.files.layout": "tree",
  "security.workspace.trust.untrustedFiles": "open",
  "git.enableSmartCommit": true,
  "remote.SSH.defaultExtensions": ["gitpod.gitpod-remote-ssh"],
  "sonarlint.pathToNodeExecutable": "/Users/siraj/.nvm/versions/node/v16.18.1/bin/node",
  "gitlens.views.branches.branches.layout": "list",
  "editor.minimap.enabled": false,
  "svelte.enable-ts-plugin": true,
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode",
  },
  "git.openRepositoryInParentFolders": "never",
  "workbench.editorAssociations": {
    "*.db": "sqlite-viewer.option",
  },
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "always",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.autoImportUserSymbols": true,
  "camelk.integrations.runtimeVersion": "1.11.0",
  "[java]": {
    "editor.defaultFormatter": "redhat.java",
  },
  "testing.autoRun.delay": 10,
  "testing.followRunningTest": false,
  "testing.openTesting": "neverOpen",
  "debug.onTaskErrors": "showErrors",
  "java.debug.settings.showStaticVariables": true,
  "java.debug.settings.showQualifiedNames": false,
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.symbolCacheDownload": true,
  "julia.enableTelemetry": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnSave": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
  },
  "java.compile.nullAnalysis.mode": "disabled",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.fullReload": true,
  "css.lint.unknownAtRules": "ignore",
  "fiveServer.injectBody": true,
  "tailwindCSS.emmetCompletions": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "right",
  },
  "notebook.undoRedoPerCell": false,
  "notebook.showFoldingControls": "always",
  "workbench.settings.useSplitJSON": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
  },
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "editor.linkedEditing": true,
  "codesnap.shutterAction": "copy",
  "codesnap.target": "window",
  "codesnap.showWindowTitle": true,
  "codesnap.showWindowControls": false,
  "codesnap.realLineNumbers": true,
  "notebook.output.wordWrap": true,
  "notebook.formatOnCellExecution": true,
  "python.formatting.provider": "none",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
  },
  "notebook.stickyScroll.enabled": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "cody.commandCodeLenses": true,
  "editor.largeFileOptimizations": false,
  "cody.autocomplete.enabled": false,
  "prisma.showPrismaDataPlatformNotification": false,
  "[powerquery]": {
    "editor.defaultFormatter": "PowerQuery.vscode-powerquery",
  },
  "notebook.output.scrolling": true,
  "interactiveWindow.executeWithShiftEnter": true,
  "notebook.output.textLineLimit": 15,
  "cursor.cpp.disabledLanguages": ["scminput"],
  "terminal.integrated.inheritEnv": false,
  "jupyter.askForKernelRestart": false,
  "notebook.outline.showCodeCellSymbols": false,
  "ruff.enable": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "Cargo.toml": "Cargo.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*",
  },
  "cursor.aipreview.enabled": true,
  "cursor.cpp.enablePartialAccepts": true,
  "testing.automaticallyOpenTestResults": "neverOpen",
  "cursorpyright.analysis.autoImportCompletions": true,
  "editor.accessibilitySupport": "on",
  "dbcode.connections": [],
  "[elixir]": {
    "editor.defaultFormatter": "elixir-lsp.elixir-ls",
  },
  "cursor.composer.usageSummaryDisplay": "always",
  "diffEditor.renderSideBySide": false,
  "amp.url": "https://ampcode.com/",
  "amp.permissions": [
    {
      "tool": "Bash",
      "action": "ask",
      "matches": {
        "cmd": ["*rm*-rf*", "*rm*-r*--force*", "*rm*--force*-r*"],
      },
    },
    {
      "tool": "Bash",
      "action": "ask",
      "matches": {
        "cmd": ["*find*-exec*", "*find*-delete*"],
      },
    },
    {
      "tool": "Bash",
      "action": "ask",
      "matches": {
        "cmd": ["*git*push*--force*", "*git*push*-f*"],
      },
    },
    {
      "tool": "Bash",
      "action": "ask",
      "matches": {
        "cmd": ["sudo*", "su*", "*chmod*777*", "*chown*root*"],
      },
    },
    {
      "tool": "*",
      "action": "allow",
      "matches": {},
    },
  ],
  "markdown.preview.fontSize": 13,
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome",
  },
  "amp.mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest", "--headless", "--isolated"],
    },
  },
  "claudeCode.preferredLocation": "panel",
  "workbench.colorTheme": "Default Light+",
}
